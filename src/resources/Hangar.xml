			<length>30</length>
			<width>23</width>
			<height>8</height>
		</parameters>
		<price currency="thaler">1200000</price>
	</plane>
	<plane model="Chi-5">
		<origin>China</origin>
		<chars>
			<type>UAV</type>
			<seats>0</seats>
			<radar>true</radar>
		</chars>
		<parameters measuring="m">
			<length>10</length>
