<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="Hangar.xsl"?>
<han:hangar xmlns:han="http://www.epam.by/Hangar"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.epam.by/Hangar Hangar.xsd">
	<plane model="Rus-1">
		<origin>Russia</origin>
		<chars>
			<type>fighter</type>
			<seats>1</seats>
			<radar>true</radar>
			<ammunition>
				<rockets>5</rockets>
				<machine_guns>2</machine_guns>
